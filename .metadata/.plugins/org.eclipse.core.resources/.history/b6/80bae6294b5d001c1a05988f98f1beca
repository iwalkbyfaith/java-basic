package game.inheritance;   // ● 정의부 ● (Commoner의 상속)

public class Warrior extends Commoner {
	
	private int pDam; // 물리 데미지
	
	
	public Warrior(String id) {
		super(id);
		this.pDam = 8;
	}
	
	
	
	public void getinfo() {
		System.out.println("레벨 : " + lv + 
						   ", 체력 : " + hp + 
						   ", 마나 : " + mp +
						   ", 물리 데미지 : " + pDam);
		
	}
}
