package accessmodifier;

public class Warrior {
	// hp, mp, lv, atk, def, exp, name;
	// 선언
	
	public String name;   // 이름
	public int hp;
	public int mp;
	public int lv;
	public int atk;
	public int def;
	public int exp;
	
	// 생성자
	
	public Warrior(String n) {
		hp = 20;
		mp = 10;
		lv = 1;
		atk = 3;
		def = 0;
		exp = 0;
			
	}
	
}
