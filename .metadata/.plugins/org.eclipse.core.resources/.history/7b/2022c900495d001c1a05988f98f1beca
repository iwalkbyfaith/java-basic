package game.inheritance;   // ● 정의부 ●
 
public class Main01 {
	public static void main(String[] args) {
		
		// 초보자 계정을 하나 생성하고 정보를 조회해보세요.
		
		Commoner c1 = new Commoner("왕초보");  // 왼쪽 Commoner 자료
											 // 오른쪽 Commoner 생성자 호출
											 // 헷갈리면 컨트롤 + 마우스 왼쪽 눌러서 넘어가봐
		c1.getInfo();
		
		Warrior w1 = new Warrior("왕초보전사");
		w1.getinfo();
	}
}
