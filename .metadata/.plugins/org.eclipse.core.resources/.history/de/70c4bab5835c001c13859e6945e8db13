package overloading;

public class Calculator {
	
	
	// 이름은 똑같은데 구별이 됨.
	
	public void plus(int a) {  // 파라미터값으로 int a만 요구
		System.out.println(a + "에 1을 더한 값은" + (a + 1) + "입니다.");
	}
	
	
	public void plus(int a, int b) { // 파라미터값으로 int a, int b만 요구
		System.out.println(a + "에 " + b + "를 더한 값은 " + (a + b) + "입니다.");
	}
	
	
}
