package interaction;

public class Main01 {
	public static void main(String[] args) {
		
		
		Seller s1 = new Seller(10);
		Seller s2 = new Seller(5);
		Buyer b1 = new Buyer(20000);
		
		s1.showSeller();
		s2.showSeller();
		b1.showBuyer();
		
		b1.buyMango(s2, 5);
		System.out.println("=============s2에게 구매==========");
		
		s1.showSeller();
		s2.showSeller();
		b1.showBuyer();		
		
		

		
	}
}
