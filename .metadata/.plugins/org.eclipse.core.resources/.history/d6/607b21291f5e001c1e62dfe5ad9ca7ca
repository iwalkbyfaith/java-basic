package game.interaction;

public class Archer {
	
	private int hp;
	private int mp;
	private int atk;
	private int def;
	private int exp;
	
	
	public Archer() {
		this.hp = 15;
		this.mp = 20;
		this.atk = 6;
		this.def = 3;
		this.exp = 0;
	}
	
	public void huntOrc(Orc orc) {
		orc.doBattle(this.atk);
	}

}
